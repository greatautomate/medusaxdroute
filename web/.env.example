# Feature Flags
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_OAUTH=true
VITE_ENABLE_TOPUP=true

# Theme Configuration
VITE_PRIMARY_COLOR="#4F46E5"
VITE_THEME_MODE="auto"

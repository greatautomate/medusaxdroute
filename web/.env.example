# MedusaXD API Frontend Environment Variables
# Copy this file to .env and customize the values

# System Branding
VITE_SYSTEM_NAME="MedusaXD API"
VITE_SYSTEM_DESCRIPTION="Next-generation AI gateway and asset management system"
VITE_SYSTEM_KEYWORDS="AI,API,Gateway,LLM,OpenAI,Claude,Gemini"
VITE_FOOTER_TEXT="Powered by MedusaXD API"

# API Configuration
VITE_REACT_APP_SERVER_URL=""

# Branding URLs
VITE_LOGO_URL=""
VITE_GITHUB_URL="https://github.com/MedusaXD/medusaxd-api"
